mostlyclean-compile:
	-rm -f *.$(OBJEXT)

distclean-compile:
	-rm -f *.tab.c

include ./$(DEPDIR)/streams_mknod-streams_mknod.Po
include ./$(DEPDIR)/autopush-autopush.Po
include ./$(DEPDIR)/fattach-fattach.Po
include ./$(DEPDIR)/fdetach-fdetach.Po
