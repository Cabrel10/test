	"$(DESTDIR)$(pkglibexecdir)" "$(DESTDIR)$(sbindir)" \
	"$(DESTDIR)$(initrddir)" "$(DESTDIR)$(pkglibexecdir)" \
	"$(DESTDIR)$(sbindir)" "$(DESTDIR)$(configdir)" \
	"$(DESTDIR)$(modutildir)" "$(DESTDIR)$(sysconfdir)" \
	"$(DESTDIR)$(pkglibexecdir)"
