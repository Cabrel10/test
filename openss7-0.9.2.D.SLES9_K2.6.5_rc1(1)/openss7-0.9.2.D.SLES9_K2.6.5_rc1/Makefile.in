	list='$(SUBDIRS)' ; for subdir in $$list ; do \
	     if test "$$subdir" = "." ; then dot_seen=yes ; fi ; \
	done ; \
	if test "$$dot_seen" = "no" ; then list="$$list ." ; fi ; \
	for subdir in $$list ; do \
