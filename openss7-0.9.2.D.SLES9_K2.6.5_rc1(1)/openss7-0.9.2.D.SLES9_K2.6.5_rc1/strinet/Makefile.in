# This below is pretty obfuscated, but it is what we need to rebujild a deb once for LiS and another
# time for Linux Fast-STREAMS.  When we drop LiS support, this can be cleaned out.  This uses the
# sneaky GNU make trick that per-target variables can be defined for the target and its dependents,
# in our case the single build target.  Packages that do not build for STREAMS should define
# conditionals WITH_LIS and WITH_LFS to be false and the plain rule above will be used instead.
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@one-deb:
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@@WITH_LIS_FALSE@@WITH_LFS_FALSE@	$(MAKE) $(AM_MAKEFLAGS) -- DEBXOPTS='$(DEBXOPTS)' DEBBOPTS='-B' BUILD_CFGOPTIONS='--disable-indep' AM_BINARY_TARGET='binary-arch' each-deb

@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@lis-deb:
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@@WITH_LIS_TRUE@	$(MAKE) $(AM_MAKEFLAGS) -- DEBXOPTS='$(DEBXOPTS)' DEBBOPTS='-B' BUILD_CFGOPTIONS='--with-lis --without-lfs --disable-indep' AM_BINARY_TARGET='binary-lis' each-deb

@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@lfs-deb:
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@@WITH_LFS_TRUE@	$(MAKE) $(AM_MAKEFLAGS) -- DEBXOPTS='$(DEBXOPTS)' DEBBOPTS='-B' BUILD_CFGOPTIONS='--with-lfs --without-lis --disable-indep' AM_BINARY_TARGET='binary-lfs' each-deb

@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@noa-deb:
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@	$(MAKE) $(AM_MAKEFLAGS) -- DEBXOPTS='$(DEBXOPTS)' DEBBOPTS='-b' BUILD_CFGOPTIONS='--disable-arch' AM_BINARY_TARGET='binary-indep' each-deb

@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@chg-deb:
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@	@if test "$(PACKAGE_DEBTOPDIR)" != "$(debbuilddir)" ; then \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@		list="$(debian_binary_files) $(debian_change_files)" ; for f in $$list ; do \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			test -f $$f || continue ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			t=`echo $$f | sed -e 's|^$(debbuilddir)/|$(PACKAGE_DEBTOPDIR)/|'` ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			if test ":$(FORCE)" = :force -o \( ! -f "$$t" -o "$$t" -ot "$$f" \) ; then \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@				$(ECHO) "cp -f -- $$f $$t" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@				cp -f -- $$f $$t ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			fi ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@		done ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@	fi

#! debs:
#!     This target will build the debian binary package (.deb file) from the
#!     .dsc created above.  (This target will also create the .dsc if it has not
#!     been created already.)  The subpackage .deb files will be named:
#!     @PACKAGE_LCNAME@-*_@VERSION@-@PACKAGE_DEBRELEASE@_*.deb, where the stars indicate the subpackage and
#!     the architecture.
#!
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@debs: lis-deb lfs-deb one-deb noa-deb chg-deb

#! csig:
#!     This target signs the .dsc file.  You will be prompted for a password,
#!     unless to specify it to make with the GNUPGPASS variable.
#!
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@csig:
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@	@list="$(debtop_change_files)" ; for f in $$list ; do \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@		test -f $$f || continue ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@		t=`echo $$f | sed -r -e 's|\.changes(\.asc)?$$|.changes.asc|'` ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@		if test ! -f $$t -o $$t -ot $$f ; then \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			user="$(GNUPGUSER)" ; user="$${user:+ -u $$user}" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			home="$(GNUPGHOME)" ; home="$${home:+ --homedir $$home}" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			pass="$(GNUPGPASS)" ; pass="$${pass:+ --batch }" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			pipe="$(GNUPGPASS)" ; pipe="$${pipe:+ $(ECHO) "$$pipe" |}" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			$(ECHO) "rm -f $$t" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			rm -f $$t ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			$(ECHO) "$(GPG) --clearsign -a --passphrase-fd 0$$user$$home$$pass -o- -- $$f > $$t" ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			eval "$$pipe $(GPG) --clearsign -a --passphrase-fd 0$$user$$home$$pass -o- -- $$f > $$t" || { rm -f $$f $$t ; exit 1 ; } ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			cp -fp $$t $$f ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@			touch $$t ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@		fi ; \
@BUILD_DPKG_TRUE@@MAINTAINER_MODE_TRUE@	done

