@CONFIG_STREAMS_BUFMOD_TRUE@	module	bufmod		bufmod
@CONFIG_STREAMS_NULLMOD_TRUE@	module	nullmod		nullmod
@CONFIG_STREAMS_PIPEMOD_TRUE@	module	pipemod		pipemod
@CONFIG_STREAMS_CONNLD_TRUE@	module	connld		connld
@CONFIG_STREAMS_SC_TRUE@	module	sc		sc
@CONFIG_STREAMS_TESTMOD_TRUE@	module	testmod		testmod
