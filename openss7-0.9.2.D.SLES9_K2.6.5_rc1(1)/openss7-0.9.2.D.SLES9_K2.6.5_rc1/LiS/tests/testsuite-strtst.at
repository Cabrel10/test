m4_divert_push([DEFAULTS])
function test_strtst() {
    if test `id -u` -ne 0 ; then return 77 ; fi
    strtst $1
    return $?
}
m4_divert_pop([DEFAULTS])

AT_TESTED([strtst])
AT_BANNER([LiS STREAMS test])

AT_SETUP([strtst open])
AT_KEYWORDS([strtst open])
AT_CHECK([test_strtst open], [], [ignore], [], [], [])
AT_CLEANUP

