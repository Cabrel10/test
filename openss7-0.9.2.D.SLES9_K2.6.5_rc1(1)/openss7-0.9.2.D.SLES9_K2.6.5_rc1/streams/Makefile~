#		$(ECHO) "$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS)' -- $$f" ; \
#		$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS)' -- $$f ; \
#	} ;  \
#	d= ; test -f $$f || d='$(srcdir)/' ; \
#	test "$@" -ef "$$d$$f" && exit 0 ; \
#	test ":$(FORCE)" != :force -a \( -f "$@" -a "$@" -nt "$$d$$f" \) || { \
#		$(ECHO) "cp -f -- $$d$$f $@" ; \
#		cp -f -- $$d$$f $@ ; \
