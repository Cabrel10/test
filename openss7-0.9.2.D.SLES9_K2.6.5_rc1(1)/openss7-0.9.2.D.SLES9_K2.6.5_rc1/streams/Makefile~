#resign: ALL_EXPORT_OPTS = RPMXOPTS='$(RPMXOPTS)'
#resign: sign
#	@rpms="`find $(rpmdir) -name '$(redhat_binary_file)' 2>/dev/null`" ; \
#	if test -z "$$rpms" ; then \
#		$(ECHO) "$(MAKE) $(AM_MAKEFLAGS) $(ALL_EXPORT_OPTS) rebuild" ; \
#		$(MAKE) $(AM_MAKEFLAGS) $(ALL_EXPORT_OPTS) rebuild ; \
#		rpms="`find $(rpmdir) -name '$(redhat_binary_file)' 2>/dev/null`" ; \
#		if test -z "$$rpms" ; then \
