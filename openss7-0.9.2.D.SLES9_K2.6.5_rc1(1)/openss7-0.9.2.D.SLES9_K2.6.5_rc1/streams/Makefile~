#			unsigned_rpms="$$unsigned_rpms $$rpm" ; \
#		else \
#			$(ECHO) "D: existing gpg sig in $$rpm" ; \
#		fi ; \
#	done ; \
#	if test ":$$unsigned_rpms" != : ; then \
#		$(ECHO) "$(RPM) --addsign $(RPMFLAGS) $(RPMKOPTS) -- $$unsigned_rpms" ; \
