@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@				$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS) --define "_without_tools --without-tools"' kversion="$$k" RPMTARGET="$$t" lis-rpm lfs-rpm one-rpm || :; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@			fi ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		done ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		$(ECHO) "$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS) --define \"_without_modules --without-modules\"' RPMTARGET=\"$$t\" lis-rpm lfs-rpm one-rpm" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS) --define "_without_modules --without-modules"' RPMTARGET="$$t" lis-rpm lfs-rpm one-rpm|| :; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	done ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	$(ECHO) "$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS) --define \"_without_modules --without-modules\"' RPMTARGET=\"noarch\" noa-rpm" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	$(MAKE) $(AM_MAKEFLAGS) RPMXOPTS='$(RPMXOPTS) --define "_without_modules --without-modules"' RPMTARGET="noarch" noa-rpm || :

#! resign:
#!     This target will search out and sign, with a GPG signature, the source
#!     rpm, and all of the binary rpms for this package that can be found in the
#!     package distribution directory.  This target will prompt for a GPG
#!     password.  Automated or unattended builts can be acheived with the
#!     `emake' expect script loccated here: `@abs_srcdir@/scripts/emake'.
#!
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@resign: ALL_EXPORT_OPTS = RPMXOPTS='$(RPMXOPTS)'
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@resign: sign
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	@rpms="`find $(rpmdir) -name '$(redhat_binary_file)' 2>/dev/null`" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	if test -z "$$rpms" ; then \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		$(ECHO) "$(MAKE) $(AM_MAKEFLAGS) $(ALL_EXPORT_OPTS) rebuild" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		$(MAKE) $(AM_MAKEFLAGS) $(ALL_EXPORT_OPTS) rebuild ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		rpms="`find $(rpmdir) -name '$(redhat_binary_file)' 2>/dev/null`" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		if test -z "$$rpms" ; then \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@			exit 1 ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		fi ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	fi ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	unsigned_rpms= ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	for rpm in $$rpms ; do \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		if ! $(RPM) -K -- $$rpm 2>&1 | grep -q ' gpg OK' >/dev/null 2>&1 ; then \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@			unsigned_rpms="$$unsigned_rpms $$rpm" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		else \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@			$(ECHO) "D: existing gpg sig in $$rpm" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		fi ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	done ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@	if test ":$$unsigned_rpms" != : ; then \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		$(ECHO) "$(RPM) --addsign $(RPMFLAGS) $(RPMKOPTS) -- $$unsigned_rpms" ; \
@BUILD_RPMS_TRUE@@MAINTAINER_MODE_TRUE@		$(RPM) --addsign $(RPMFLAGS) $(RPMKOPTS) -- $$unsigned_rpms ; \
