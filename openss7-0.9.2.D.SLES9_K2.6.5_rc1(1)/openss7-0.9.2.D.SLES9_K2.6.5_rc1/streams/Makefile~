# For the master build package it is sometime necessary to pass extra options down to configured
# subdirectories for a master build.  These options will contain the passed down options from the
# environment in that case.  See the recursive build targets below.
##DEBXOPTS = -D
##AM_BINARY_TARGET = binary-arch
all: config.h
	$(MAKE) $(AM_MAKEFLAGS) all-recursive

.SUFFIXES:
.SUFFIXES: .ko .o .c .ver .c .ver .lo .obj
am--refresh:
	@:
$(srcdir)/Makefile.in: # $(srcdir)/Makefile.am $(srcdir)/am/openss7.am $(srcdir)/am/help.am $(srcdir)/am/pr.am $(srcdir)/am/kernel.am $(srcdir)/am/libraries.am $(srcdir)/am/genksyms.am $(srcdir)/am/strconf.am $(srcdir)/am/init.am $(srcdir)/am/lsm.am $(srcdir)/am/archive.am $(srcdir)/am/rpm.am $(srcdir)/am/deb.am $(am__configure_deps)
	@for dep in $?; do \
	  case '$(am__configure_deps)' in \
	    *$$dep*) \
	      echo ' cd $(srcdir) && $(AUTOMAKE) --gnits '; \
	      cd $(srcdir) && $(AUTOMAKE) --gnits  \
		&& exit 0; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --gnits  Makefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnits  Makefile
.PRECIOUS: Makefile
Makefile: $(srcdir)/Makefile.in $(top_builddir)/config.status
	@case '$?' in \
	  *config.status*) \
	    echo ' $(SHELL) ./config.status'; \
	    $(SHELL) ./config.status;; \
	  *) \
	    echo ' cd $(top_builddir) && $(SHELL) ./config.status $@ $(am__depfiles_maybe)'; \
	    cd $(top_builddir) && $(SHELL) ./config.status $@ $(am__depfiles_maybe);; \
	esac;

$(top_builddir)/config.status: $(top_srcdir)/configure $(CONFIG_STATUS_DEPENDENCIES)
	$(SHELL) ./config.status --recheck

$(top_srcdir)/configure: # $(am__configure_deps)
	cd $(srcdir) && $(AUTOCONF)
$(ACLOCAL_M4): # $(am__aclocal_m4_deps)
	cd $(srcdir) && $(ACLOCAL) $(ACLOCAL_AMFLAGS)

config.h: stamp-h1
	@if test ! -f $@; then \
	  rm -f stamp-h1; \
	  $(MAKE) stamp-h1; \
	else :; fi

stamp-h1: $(srcdir)/config.h.in $(top_builddir)/config.status
	@rm -f stamp-h1
	cd $(top_builddir) && $(SHELL) ./config.status config.h
$(srcdir)/config.h.in: # $(am__configure_deps) 
	cd $(top_srcdir) && $(AUTOHEADER)
	rm -f stamp-h1
	touch $@

include/sys/streams/config.h: include/sys/streams/stamp-h2
	@if test ! -f $@; then \
	  rm -f include/sys/streams/stamp-h2; \
	  $(MAKE) include/sys/streams/stamp-h2; \
	else :; fi

include/sys/streams/stamp-h2: $(top_srcdir)/include/sys/streams/config.h.in $(top_builddir)/config.status
	@rm -f include/sys/streams/stamp-h2
	cd $(top_builddir) && $(SHELL) ./config.status include/sys/streams/config.h

distclean-hdr:
	-rm -f config.h stamp-h1 include/sys/streams/config.h include/sys/streams/stamp-h2
