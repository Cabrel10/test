	   $(INSTALL_PROGRAM_ENV) $(LIBTOOL) --mode=install $(pkglibexecPROGRAMS_INSTALL) "$$p" "$(DESTDIR)$(pkglibexecdir)/$$f" || exit 1; \
	  else :; fi; \
	done

