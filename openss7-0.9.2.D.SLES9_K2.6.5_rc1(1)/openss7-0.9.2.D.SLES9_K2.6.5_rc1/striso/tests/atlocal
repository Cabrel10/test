at_test=`../scripts/config.guess`
at_test_alias=
at_test_cpu=`echo $at_test | sed -e 's|-.*||g'`
at_test_vendor=`echo $at_test | sed -e 's|^[^-]*-||;s|-.*||g'`
at_test_os=`echo $at_test | sed -e 's|^[^-]*-[^-]*-||'`

#echo "at_test          =\"$at_test\""
#echo "at_test_alias    =\"$at_test_alias\""
#echo "at_test_cpu      =\"$at_test_cpu\""
#echo "at_test_os       =\"$at_test_os\""
#echo "at_test_vendor   =\"$at_test_vendor\""

prefix="/usr"
exec_prefix="${prefix}"
at_bindir="${exec_prefix}/bin"
at_datadir="${prefix}/share"
at_DESTDIR="${DESTDIR}"
at_exec_prefix="${prefix}"
at_kmoduledir="${rootdir}/lib/modules/${kversion}"
at_rootdir="${rootdir}"
at_ksysmap="/boot/System.map-2.6.5-7.282-smp"
at_kversion="2.6.5-7.282-smp"
at_libdir="${exec_prefix}/lib"
at_libexecdir="${exec_prefix}/libexec"
at_localstatedir="${prefix}/var"
at_mkdir_p="mkdir -p --"
at_oldincludedir="/usr/include"
at_prefix="/usr"
at_sbindir="${exec_prefix}/sbin"
at_sharedstatedir="${prefix}/com"
at_sysconfdir="${rootdir}/etc"

