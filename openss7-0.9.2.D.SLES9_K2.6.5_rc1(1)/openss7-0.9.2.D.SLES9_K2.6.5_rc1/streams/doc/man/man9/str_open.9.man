.TH STR_OPEN 9 "@PACKAGE_DATE@" "@PACKAGE@-@VERSION@" "@PACKAGE_TITLE@ DDI/DKI"
.\"
.\"
.SH NAME
.B str_open, str_close, strrput, strwput, strwsrv
\- Stream head module procedures
.\"
.\"
.SH SYNOPSIS
.PP
.B #include <sys/stream.h>
.br
.B #include <sys/strsubr.h>
.HP
.BI "int str_open(queue_t *" q ", dev_t *" devp ", int " oflag ", int " sflag ", cred_t *" crp ");
.PD 0
.HP
.BI "int str_close(queue_t *" q ", int " oflag ", cred_t *" crp ");
.HP
.BI "int strrput(queue_t *" q ", mblk_t *" mp ");
.HP
.BI "int strwput(queue_t *" q ", mblk_t *" mp ");
.HP
.BI "int strwsrv(queue_t *" q ");
.PD
.\"
.\"
.SH ARGUMENTS
.PP
Standard.
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH INTERFACE
.PP
.IR STREAMS ,
implementation specific.
.\"
.\"
.SH DESCRIPTION
.PP
.BR str_open (),
.BR str_close (),
.BR strrput (),
.BR strwput (),
.BR strwsrv (),
specify the operation of the Stream head module,
.BR sth (4).
.\"
.\"
.SH USAGE
.PP
.BR str_open (),
.BR str_close (),
.BR strrput (),
.BR strwput (),
.BR strwsrv (),
are internal
.B "@PACKAGE_TITLE@"
implementations of Stream head module procedures that are exported for use in Stream head
replacements and not intended to be used by
.I STREAMS
drivers or modules.
.\"
.\"
.SH RETURN
.PP
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH ERRORS
.PP
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH CONTEXT
.PP
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH MP-STREAMS
.PP
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH NOTICES
.PP
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH IMPLEMENTATION
.PP
See
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH "SEE ALSO"
.PP
.IR qi_qopen (9),
.IR qi_qclose (9),
.IR qi_putp (9),
.IR qi_srvp (9).
.\"
.\"
.SH BUGS
.PP
.BR str_open (),
.BR str_close (),
.BR strrput (),
.BR strwput (),
.BR strwsrv ()
have no known bugs.
.\"
.\"
.SH COMPATIBILITY
.PP
.BR str_open (),
.BR str_close (),
.BR strrput (),
.BR strwput (),
.BR strwsrv ()
are compatible with
.IR "SVR 4.2 MP DDI/DKI" ,
and systems based on
.IR "SVR 4" ,
with the following portability considerations:
.IP \(em \w'\(em\(em'u
.IP \(em
.\"
.PP
See
.BR STREAMS (9)
for more compatibility information.
.\"
.\"
.SH CONFORMANCE
