install-dist_initrdSCRIPTS: $(dist_initrd_SCRIPTS)
	@$(NORMAL_INSTALL)
	test -z "$(initrddir)" || $(mkdir_p) "$(DESTDIR)$(initrddir)"
	@list='$(dist_initrd_SCRIPTS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(dist_initrdSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(initrddir)/$$f'"; \
	    $(dist_initrdSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(initrddir)/$$f"; \
	  else :; fi; \
	done

uninstall-dist_initrdSCRIPTS:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_initrd_SCRIPTS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  echo " rm -f '$(DESTDIR)$(initrddir)/$$f'"; \
	  rm -f "$(DESTDIR)$(initrddir)/$$f"; \
	done

installcheck-dist_initrdSCRIPTS: $(dist_initrd_SCRIPTS)
	bad=0; pid=$$$$; list="$(dist_initrd_SCRIPTS)"; for p in $$list; do \
	  case ' $(AM_INSTALLCHECK_STD_OPTIONS_EXEMPT) ' in \
	   *" $$p "* | *" $(srcdir)/$$p "*) continue;; \
	  esac; \
	  f=`echo "$$p" | sed 's,^.*/,,;$(transform)'`; \
	  for opt in --help --version; do \
	    if "$(DESTDIR)$(initrddir)/$$f" $$opt >c$${pid}_.out \
	         2>c$${pid}_.err </dev/null \
		 && test -n "`cat c$${pid}_.out`" \
		 && test -z "`cat c$${pid}_.err`"; then :; \
	    else echo "$$f does not support $$opt" 1>&2; bad=1; fi; \
	  done; \
	done; rm -f c$${pid}_.???; exit $$bad
